<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<document>
	<mscreen>
		<configuration device="CIRCLE_360x360" id="config1"
			name="common" orientation="portrait" type="common" />
	</mscreen>
	<views startup="view1">
		<view id="view1" indicator="true" type="view">
			<box homogeneous="false" align_h="fill" align_v="fill" visible="true"
				weight_v="1" padding_v="0" id="box1" padding_h="0" weight_h="1"
				direction="vertical">
				<label align_h="fill" wrap_width="0" align_v="fill" visible="true"
					weight_v="1" line_wrap="none" id="lblTitle"
					text="&lt;align='center'&gt;IoTivity&lt;align&gt;" weight_h="1"
					ellipsis="true"></label>
				<label align_h="fill" wrap_width="0" align_v="fill" visible="true"
					weight_v="1" line_wrap="none" id="lblTime"
					text="&lt;align='center'&gt;00:00:00&lt;/align&gt;" weight_h="1"
					ellipsis="true"></label>
				<label align_h="fill" wrap_width="0" align_v="fill" visible="true"
					weight_v="1" line_wrap="none" id="lblheartrate"
					text="&lt;align='center'&gt;Heartrate:&lt;/align&gt;" weight_h="1"
					ellipsis="true"></label>
				<button align_h="fill" align_v="fill" visible="true"
					weight_v="1" disable="false" style="default" id="button1" text="Refresh"
					weight_h="1">
					<event function_name="view1_button1_onclicked" signal="clicked" />
				</button>
			</box>
		</view>
	</views>
</document>
